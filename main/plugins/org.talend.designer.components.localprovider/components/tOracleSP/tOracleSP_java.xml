	         <ITEMS DEFAULT="AUTOMAPPING">
	            <ITEM NAME="AUTOMAPPING" VALUE="AUTOMAPPING"/>
	            <ITEM NAME="CLOB" VALUE="CLOB"/>
	            <ITEM NAME="BLOB" VALUE="BLOB"/>
	          	<ITEM NAME="DECIMAL" VALUE="DECIMAL"/>
